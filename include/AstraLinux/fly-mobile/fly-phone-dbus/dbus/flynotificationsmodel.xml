<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.fly.phone.launcher.NotificationsModel">
    <method name="addNotification">
      <arg type="o" direction="out"/>
      <arg name="type" type="i" direction="in"/>
      <arg name="removable" type="b" direction="in"/>
      <arg name="save" type="b" direction="in"/>
      <arg name="dataMap" type="a{sv}" direction="in"/>
      <annotation name="org.qtproject.QtDBus.QtTypeName.In3" value="QVariantMap"/>
    </method>
    <method name="getNotification">
      <arg type="o" direction="out"/>
      <arg name="id" type="i" direction="in"/>
    </method>
    <method name="removeNotification">
      <arg name="id" type="i" direction="in"/>
    </method>
    <method name="removeMessageNotificationsWithPhone">
      <arg name="phone" type="s" direction="in"/>
    </method>
  </interface>
</node>
