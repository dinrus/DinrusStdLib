<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.fly.phone.launcher.FlyTaskDbusNotifier">
    <signal name="taskTriggered">
      <arg name="id" type="s" direction="out"/>
      <arg name="message" type="s" direction="out"/>
    </signal>
    <signal name="taskCreated">
      <arg name="id" type="s" direction="out"/>
    </signal>
    <signal name="taskUpdated">
      <arg name="id" type="s" direction="out"/>
    </signal>
    <signal name="taskRemoved">
      <arg name="id" type="s" direction="out"/>
    </signal>
    <method name="emitTaskTriggered">
      <arg name="id" type="s" direction="in"/>
      <arg name="message" type="s" direction="in"/>
    </method>
    <method name="emitTaskCreated">
      <arg name="id" type="s" direction="in"/>
    </method>
    <method name="emitTaskUpdated">
      <arg name="id" type="s" direction="in"/>
    </method>
    <method name="emitTaskRemoved">
      <arg name="id" type="s" direction="in"/>
    </method>
  </interface>
</node>
