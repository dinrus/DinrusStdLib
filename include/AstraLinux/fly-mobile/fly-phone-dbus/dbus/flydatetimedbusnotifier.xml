<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.fly.phone.launcher.FlyDateTimeDbusNotifier">
    <signal name="systemDateTimeChanged">
    </signal>
    <signal name="timeZoneChanged">
    </signal>
    <signal name="alarmTriggered">
      <arg name="name" type="s" direction="out"/>
      <arg name="sound" type="s" direction="out"/>
      <arg name="delay" type="i" direction="out"/>
    </signal>
    <signal name="timerStarted">
        <arg name="ms" type="s" direction="out"/>
        <arg name="sound" type="s" direction="out"/>
    </signal>
    <signal name="timerPaused">
        <arg name="ms" type="s" direction="out"/>
    </signal>
    <signal name="timerResumed">
        <arg name="ms" type="s" direction="out"/>
    </signal>
    <signal name="timerStopped">
    </signal>
    <method name="emitSystemDateTimeChanged">
    </method>
    <method name="emitTimeZoneChanged">
    </method>
    <method name="emitAlarmTriggered">
      <arg name="name" type="s" direction="in"/>
      <arg name="sound" type="s" direction="in"/>
      <arg name="delay" type="i" direction="in"/>
    </method>
    <method name="emitTimerStarted">
        <arg name="ms" type="s" direction="in"/>
        <arg name="sound" type="s" direction="in"/>
    </method>
    <method name="emitTimerPaused">
        <arg name="ms" type="s" direction="in"/>
    </method>
    <method name="emitTimerResumed">
        <arg name="ms" type="s" direction="in"/>
    </method>
    <method name="emitTimerStopped">
    </method>
  </interface>
</node>
